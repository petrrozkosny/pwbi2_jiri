dim_datumy = 
ADDCOLUMNS(
SELECTCOLUMNS(
    CALENDARAUTO(),"datum",[Date]),
"rok",YEAR([datum]),
"kvartal",QUARTER([datum]),
"mesic",MONTH([datum]),
"cislo_tydne",WEEKNUM([datum],21))

