ceske_lokality = 
-- SELECT dim_lokality[Lokalita] FROM dim_lokality
-- WHERE dim_lokality[Zeme] = "Cesko"
CALCULATETABLE(
    VALUES(dim_lokality[Lokalita]),
dim_lokality[Zeme] = "Cesko")
